{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-openssl": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "aws/aws-sdk-php": "^3.369",
        "barryvdh/laravel-dompdf": "^3.0",
        "brian2694/laravel-toastr": "^5.57",
        "cboden/ratchet": "^0.4.4",
        "gregwar/captcha": "1.*",
        "guzzlehttp/guzzle": "^7.0.1",
        "iyzico/iyzipay-php": "^2.0",
        "jenssegers/agent": "^2.6",
        "kreait/firebase-php": "^7.0",
        "laravel/framework": "^12.0",
        "laravel/passport": "^12.0",
        "laravel/reverb": "^1.4.3",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.10.1",
        "madnest/madzipper": "^1.4",
        "matanyadaev/laravel-eloquent-spatial": "^4.5.0",
        "mercadopago/dx-php": "2.4.3",
        "mpdf/mpdf": "^8.2",
        "nwidart/laravel-modules": "^12.0",
        "openai-php/laravel": "^0.17.1",
        "pusher/pusher-php-server": "^7.2",
        "rap2hpoutre/fast-excel": "^5.3",
        "razorpay/razorpay": "^2.9",
        "react/promise": "^3.1",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/db-dumper": "^3.4",
        "stevebauman/location": "^7.1",
        "stripe/stripe-php": "^13.9",
        "twilio/sdk": "^7.14",
        "xendit/xendit-php": "^4.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/"
        },
        "exclude-from-classmap": [
            "Modules/*/Database/Migrations"
        ],
        "files": [
            "app/Lib/Helpers.php",
            "app/Lib/ReverbPusherHelpers.php",
            "app/Lib/Constant.php",
            "app/Lib/Response.php",
            "Modules/ZoneManagement/Lib/Zone.php",
            "Modules/PromotionManagement/Lib/PromotionNew.php",
            "Modules/BusinessManagement/Lib/BusinessSettingType.php",
            "Modules/Gateways/Library/Constant.php",
            "app/Library/TripRequestUpdate.php",
            "app/Library/CustomerWalletUpdate.php",
            "app/Library/DriverDigitalPay.php",
            "Modules/AiModule/Lib/Response.php",
            "Modules/AiModule/Lib/Helper.php",
            "Modules/BlogManagement/Lib/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
